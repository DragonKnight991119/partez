{
    "name": "abollinger/partez",
    "type": "project",
    "description": "A simple & fast PHP starter kit for web app",
    "homepage": "https://abollinger.fr/partez/",
    "license": "MIT",
    "require": {
        "twig/twig": "^3.4",
        "twig/html-extra": "^3.4",
        "twig/intl-extra": "^3.4",
        "symfony/yaml": "^6.1",
        "erusev/parsedown": "^1.7",
        "symfony/dotenv": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Abollinger\\Partez\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:8000"
        ],
        "bricolo": "php bricolo/bricolo.php"
    },
    "authors": [
        {
            "name": "Antoine Bollinger",
            "email": "antoine.bollinger@gmail.com",
            "homepage": "https://antoinebollinger.fr",
            "role": "Developer"
        }
    ]
}