{
    "name": "abollinger/php-starter",
    "type": "project",
    "description": "A starting kit for small php app",
    "version": "1.1.3",
    "homepage": "https://abollinger.fr/php-starter/",
    "license": "MIT",
    "require": {
        "twig/twig": "^3.4",
        "twig/html-extra": "^3.4",
        "twig/intl-extra": "^3.4",
        "symfony/yaml": "^6.1",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Abollinger\\PHPStarter\\": "src/"
        }
    },
    "scripts": {
        "serve": "php -S localhost:8000",
        "bricolo": "php config/Bricolo.class.php"
    },
    "config": {
        "process-timeout": 0
    },
    "authors": [
        {
            "name": "Antoine Bollinger",
            "email": "antoine.bollinger@gmail.com",
            "homepage": "https://antoinebollinger.fr",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org/packages/abollinger/php-starter"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Antoine-Bollinger/php-starter"
        }
    ]
}